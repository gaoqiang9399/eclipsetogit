			  break;
			case 3:
				resDispaly("#freewall",disParm3);
				showVal(disParm3);
			  break;
			default:
				break;
			}
			wall.filter(".cell-dis");
			wall.fitWidth();
		}
		function showVal(obj){
			$(".paramVal").html("当前参数："+JSON.stringify(obj));
		}
