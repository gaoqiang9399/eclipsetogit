				selector: '.cell',
				animate: true,
				onResize: function() {
					wall.fitWidth();
				}
			});
			switch(type)
			{
			case 0:
				resDispaly("#freewall",{});
				showVal({});
			  break;
			case 1:
				resDispaly("#freewall",disParm1);
				showVal(disParm1);
			  break;
			case 2:
				resDispaly("#freewall",disParm2);
